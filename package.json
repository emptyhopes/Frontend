{
  "private": true,
  "name": "frontend-spa",
  "scripts": {
    "watch": "webpack serve --config Application/Ship/Utils/Webpack/Watch/webpack.watch.js",
    "development": "webpack --config Application/Ship/Utils/Webpack/Development/webpack.development.js",
    "production:spa": "webpack --config Application/Ship/Utils/Webpack/Production/SPA/webpack.production.js",
    "production:ssr": "webpack --config Application/Ship/Utils/Webpack/Production/SSR/webpack.production.js",
    "development:docker": "docker-compose --file Application/Ship/Utils/Docker/Development/docker-compose.development.yml up --build --detach",
    "production:spa:docker": "docker-compose --file Application/Ship/Utils/Docker/Production/SPA/docker-compose.production.yml up --build --detach",
    "production:ssr:docker": "docker-compose --file Application/Ship/Utils/Docker/Production/SSR/docker-compose.production.yml up --build --detach",
    "development:docker:remove": "docker-compose --file Application/Ship/Utils/Docker/Development/docker-compose.development.yml rm --stop --force && docker system prune --all --force --volumes",
    "production:spa:docker:remove": "docker-compose --file Application/Ship/Utils/Docker/Production/SPA/docker-compose.production.yml rm --stop --force && docker system prune --all --force --volumes",
    "production:ssr:docker:remove": "docker-compose --file Application/Ship/Utils/Docker/Production/SSR/docker-compose.production.yml rm --stop --force && docker system prune --all --force --volumes",
    "stats": "webpack-bundle-analyzer Build/stats.json",
    "prettier:write": "prettier --write .",
    "stylelint:styled-components": "stylelint \"**/*.(jsx|tsx)\" --config Application/Ship/Utils/Stylelint/StyledComponents/.stylelintrc",
    "stylelint:styles": "stylelint \"**/*.(css|scss|sass)\" --config Application/Ship/Utils/Stylelint/Styles/.stylelintrc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tests": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/mocha": "^10.0.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cssnano": "^5.1.12",
    "eslint": "^8.27.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "sass": "^1.54.2",
    "sass-loader": "^13.0.2",
    "styled-components": "^5.3.6",
    "stylelint": "^14.9.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-scss": "^4.3.0",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.9.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.24",
    "normalize.css": "^8.0.1",
    "null-loader": "^4.0.1"
  }
}
